<%= simple_form_for(@playlist) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :mood,:label_method => :moodname, :value_method => :id%>
    <%= f.association :user, :label_method => :email, :value_method => :id %>
  	
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
