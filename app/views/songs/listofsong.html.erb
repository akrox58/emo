<!DOCTYPE html>
<html lang="en">
<head>
    <title></title>

    <!-- add styles and scripts -->
	<%=stylesheet_link_tag 'css/styles.css' %>
	<%=javascript_include_tag 'js/jquery-1.7.2.min.js' ,'js/jquery-ui-1.8.21.custom.min.js','js/man.js' %>
</head>
<body>
    <div class="example">

        <div class="player">
            <div class="pl"></div>
            <div class="title"></div>
            <div class="artist"></div>
	    <div class="mood"></div>
            <div class="cover"></div>
            <div class="controls">
                <div class="play"></div>
                <div class="pause"></div>
                <div class="rew"></div>
                <div class="fwd"></div>
            </div>
            <div class="volume"></div>
            <div class="tracker"></div>
        </div>

  


        <ul class="playlist">
	<% @songs.each do |song| %>
            <li audiourl="<%=song.audio.url%>" cover="cover2.jpg" artist="<%=song.artist.artistname%>" mood="<%=song.mood.moodname%>" ><%=song.name%> </li>
	  <% end %>        
	</ul>

    </div>
</body>
<br>
<br>
<br>
<br>
<br>
<br>
</html>


